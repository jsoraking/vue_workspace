window.onload = function(){
	var test1 = new Vue({
		el:'#test1',
		data : {
			str1: '문자1',
			int1 : 100,
			float1 : 11.11
		}
		
	});
	var test2 = new Vue({
		el:'.test2',
		data : {
			str2: '문자2',
			int2 : 200,
			float2 : 22.22
		}
		
	});
	var test2 = new Vue({
		el:'#test3',
		data : {
			int3: 100,
			float3 : 11.11,
			str3 : '문자열3',
			boolean3 : true,
			array: [10, 20, 30]
		}
		
	});
}
</script>
</head>
